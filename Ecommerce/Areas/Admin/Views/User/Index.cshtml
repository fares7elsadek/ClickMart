@{
    ViewData["Title"] = "Manage Users";
}
@Html.AntiForgeryToken()
<div class="container">
    <div class="content m-auto mt-5" style="width: 100%;">
        <div class="d-flex justify-content-between align-items-center mb-4">
            <h2 class="mb-0">Users</h2>
        </div>
        <div class="table-responsive">
            <table id="AllUserDataTable" class="table table-hover align-middle text-center" style="width:100%">
                <thead class="table-light">
                    <tr>
                        <th scope="col">Name</th>
                        <th scope="col">Username</th>
                        <th scope="col">Email</th>
                        <th scope="col">Phone number</th>
                        <th scope="col">Role</th>
                        <th scope="col">Actions</th>
                    </tr>
                </thead>
            </table>
        </div>
    </div>
</div>


@section Navbar {
    <partial name="_NavbarPartial" model="@ViewBag.User" />
}

@section Sidebar {
    <partial name="_SidbarPartial" model="@ViewBag.User" />
}
@section Scripts {
    <script src="~/js/User/DataTable.js"></script>
}
